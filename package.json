{
  "name": "utilsac",
  "version": "12.2.8",
  "description": "Utility functions",
  "license": "CC0-1.0",
  "type": "module",
  "main": "utility.js",
  "scripts": {
    "test": "ava",
    "perftest": "node --experimental-modules ./tests/performance/deepCopy.js",
    "lint-fix": "eslint --ignore-path .gitignore --fix .",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-red": "^1.4.1",
    "leistung": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "red"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {}
    },
    "env": {
      "es2020": true,  
      "browser": true
    }
  },
  "ava": {
    "files": [
      "tests/specification/**"
    ],
    "require": [
      "esm"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          [
            "module:ava/stage-4",
            {
              "modules": false
            }
          ]
        ]
      }
    },
    "compileEnhancements": false
  },
  "files": [
    "typeCast.js",
    "evalGlobal.js",
    "blobs.js",
    "utility.js",
    "deep.js",
    "readme.md",
    "license.txt",
    "changelog.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/utilsac.git"
  },
  "keywords": [
    "utility",
    "deep",
    "equal",
    "copy",
    "assign",
    "memoize",
    "type",
    "cast",
    "eval",
    "global"
  ]
}
